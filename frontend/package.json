{
  "name": "secureauditai-frontend",
  "version": "1.0.0",
  "description": "React frontend for SecureAuditAI compliance auditing platform",
  "main": "src/index.tsx",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "CI=true react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx serve -s build -l 3001"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^5.3.0",
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "aws-amplify": "^5.3.0",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^2.0.0",
    "typescript": "^4.9.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.0",
    "eslint": "^8.35.0",
    "tailwindcss": "^3.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001"
}
